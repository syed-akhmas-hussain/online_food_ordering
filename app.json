{
  "expo": {
    "name": "AkhmasOnlineFoodOrdering",
    "slug": "AkhmasOnlineFoodOrdering",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby restaurants.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to track your delivery."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.anonymous.AkhmasOnlineFoodOrdering"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
