{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "strict": true,
      "jsx": "react-native",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowJs": true,
      "noEmit": true,
      "isolatedModules": true
    },
    "extends": "expo/tsconfig.base",
    "include": ["**/*.ts", "**/*.tsx"]
  }
  